package BasicSyntaxExercises;

import java.util.Scanner;

public class Ages {
    public static void main(String[] args) {
         Scanner scanner = new Scanner(System.in);

          int a = Integer.parseInt(scanner.nextLine());

          if (a<=2){System.out.println("baby");}
         else if (a<=13){System.out.println("child");}
         else if (a<=19){System.out.println("teenager");}
         else if (a<=65){System.out.println("adult");}
         else if (a>65){System.out.println("elder");}

    }
}
